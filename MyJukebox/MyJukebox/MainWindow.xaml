<Window x:Class="MyJukebox.MainWindow"
                xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                xmlns:telerik="http://schemas.telerik.com/2008/xaml/presentation"
                Title="MyJukebox" Height="350" Width="525"
                MinWidth="300" SizeToContent="WidthAndHeight">
    <Window.CommandBindings>
        <CommandBinding Command="ApplicationCommands.Open" CanExecute="OnCanExecuteOpen" Executed="OnExecutedOpen"/>
        <CommandBinding Command="MediaCommands.Play" CanExecute="OnCanExecutePlay" Executed="OnExecutedPlay"/>
        <CommandBinding Command="MediaCommands.Pause" CanExecute="OnCanExecutePause" Executed="OnExecutedPause"/>
        <CommandBinding Command="MediaCommands.Stop" CanExecute="OnCanExecuteStop" Executed="OnExecutedStop"/>
    </Window.CommandBindings>
        <Grid>
                
        </Grid>
</Window>
